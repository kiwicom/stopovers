{
  "name": "stopovers",
  "version": "1.0.0",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "start:exported": "npx serve out -p 3000",
    "export": "next build && next export",
    "export:full": "npx serve -p 3000 & npm run export && kill $!",
    "test": "jest",
    "lint": "eslint .",
    "test-ci": "yarn lint && yarn typecheck",
    "madge": "madge -c pages/index.js",
