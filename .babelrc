{
  "plugins": [
    "transform-flow-strip-types",
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true
      }
    ]
  ],
  "env": {
    "development": {
      "presets": ["next/babel"]
    },
    "production": {
      "presets": ["next/babel"]
    },
    "test": {
      "plugins": ["@babel/plugin-proposal-class-properties"],
      "presets": ["@babel/preset-env", "@babel/preset-react"]
    }
  }
}
